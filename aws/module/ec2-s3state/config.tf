
################################################################################
# Provider
################################################################################
provider "aws" {
  region = "us-east-1"
}

################################################################################
# State File
################################################################################
terraform {
  backend "s3" {
      bucket         = "cloudopsguru-terraform"
      key            = "dev/ec2/terraform.tfstate"
      dynamodb_table = "terraform_locks"
      region         = "ap-south-1"
 }
}

 